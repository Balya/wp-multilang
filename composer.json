{
  "name": "valexar/wp-multilang",
  "description": "Multilingual plugin for WordPress.",
  "type": "wordpress-plugin",
  "license": "GPL2",
  "authors": [
    {
      "name": "Valentyn Riaboshtan",
      "email": "valexar12@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "keywords": [
    "wordpress",
    "multilingual",
    "multilang",
    "languages",
    "translation",
    "multilanguage"
  ],
  "support": {
    "issues": "https://github.com/valexar/wp-multilang/issues",
    "forum": "https://wordpress.org/support/plugin/wp-multilang",
    "wiki": "https://github.com/valexar/wp-multilang/wiki",
    "source": "https://github.com/valexar/wp-multilang"
  },
  "require": {
    "php": ">=5.6.4",
    "xrstf/composer-php52": "1.*"
  },
  "require-dev": {
    "kint-php/kint": "^2.1"
  },
  "autoload": {
    "classmap": [
      "includes/"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-update-cmd": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ],
    "post-autoload-dump": [
      "xrstf\\Composer52\\Generator::onPostInstallCmd"
    ]
  }
}
